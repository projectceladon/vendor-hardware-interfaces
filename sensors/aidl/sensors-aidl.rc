service vendor.sensors-default /apex/com.android.hardware.sensors.intel/bin/hw/android.hardware.sensors@aidl-service.intel
    interface aidl android.hardware.sensors.ISensors/default 
    class hal
    user system
    group system
    group system wakelock uhid context_hub
    capabilities BLOCK_SUSPEND
    rlimit rtprio 10 10
